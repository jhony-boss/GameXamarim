<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GameXamarim.PageJogo" BackgroundColor="#68BF8B" Title="Resultado">
    <ContentPage.Content>
        <StackLayout>
            <Image x:Name="imgMoeda" Source="moeda_cara" BackgroundColor="Transparent" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"></Image>
            <ImageButton Source="botao_voltar" BackgroundColor="Transparent" x:Name="btVoltar" Clicked="btVoltar_Clicked"></ImageButton>
            </StackLayout>
    </ContentPage.Content>
</ContentPage>